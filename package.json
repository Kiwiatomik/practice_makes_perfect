{
  "name": "practice_makes_perfect",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node scripts/generate-health-files.js",
    "build:typed": "tsc --project tsconfig.build.json && vite build",
    "build:full": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 10",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "deploy:staging": "node scripts/deploy.js staging",
    "deploy:production": "node scripts/deploy.js production",
    "deploy:staging:force": "node scripts/deploy.js staging --force",
    "deploy:production:force": "node scripts/deploy.js production --force",
    "rollback:staging": "node scripts/rollback.js staging",
    "rollback:production": "node scripts/rollback.js production",
    "rollback:emergency": "node scripts/rollback.js production --emergency",
    "docker:dev": "docker compose build && docker compose up",
    "docker:dev:detached": "docker compose build && docker compose up -d",
    "docker:dev:stop": "docker compose down",
    "docker:dev:logs": "docker compose logs -f app",
    "docker:prod": "docker compose -f docker-compose.prod.yml build && docker compose -f docker-compose.prod.yml up",
    "docker:prod:detached": "docker compose -f docker-compose.prod.yml build && docker compose -f docker-compose.prod.yml up -d",
    "docker:prod:stop": "docker compose -f docker-compose.prod.yml down",
    "docker:build": "docker build -t practice-makes-perfect .",
    "docker:clean": "docker system prune -f && docker volume prune -f"
  },
  "dependencies": {
    "@sentry/react": "^10.12.0",
    "@sentry/vite-plugin": "^4.3.0",
    "bootstrap": "^5.3.7",
    "firebase": "^12.2.1",
    "katex": "^0.16.22",
    "openai": "^5.12.2",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.0",
    "react-katex": "^3.1.0",
    "react-router": "^7.7.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-katex": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jsdom": "^26.1.0",
    "playwright": "^1.55.0",
    "sass": "^1.89.2",
    "terser": "^5.44.0",
    "typescript": "^5.2.2",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  }
}
